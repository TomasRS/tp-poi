package ar.edu.TPPOI;

import java.text.DateFormat;
import java.text.SimpleDateFormat;
import java.util.Date;

public class Servicio {
	
	private String fechaHs;
	
	public Servicio(String fechaHs){
		this.fechaHs = fechaHs;
		DateFormat df4 = new SimpleDateFormat("dd-MM-yyyy HH:mm:ss");
	
		try {
		
			Date fechaHsServicio = df4.parse(fechaHs);
	
		} catch(Exception e){
		
	    System.out.println("Error: Debe poner dicho formato de fecha y horario: dd-MM-yyyy HH:mm:ss");
		}
	}

}
